<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Authors>Matthias Hurrle</Authors>
    <Description>Search any tree-like data structure with wildcard and globbing patterns.</Description>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Copyright>Copyright (c) 2019 Matthias Hurrle</Copyright>
    <PackageProjectUrl>https://github.com/atzedent/globkit</PackageProjectUrl>
    <RepositoryUrl>https://github.com/atzedent/globkit</RepositoryUrl>
    <PackageTags>search wildcard glob globbing tree</PackageTags>
    <NeutralLanguage></NeutralLanguage>
    <PackageReleaseNotes>Search any tree-like data structure with wildcard and globbing patterns.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
